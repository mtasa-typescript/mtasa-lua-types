{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": ".",
    "typeRoots": [
      "."
    ],
    "types": [
      "typescript-to-lua/language-extensions",
    ],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  }
}